{
  "name": "volume-profiles",
  "version": "3.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "postinstall": "bun setup",
    "setup": "spicetify restore backup && spicetify enable-devtools && lefthook install",
    "postsetup": "bun setup.js",
    "upgradeDeps": "bun update",
    "audit": "bun audit",
    "lint": "biome check --write",
    "build": "spicetify-creator",
    "postbuild": "spicetify apply",
    "build-local": "spicetify-creator --out=dist --minify",
    "watch": "spicetify-creator --watch"
  },
  "dependencies": {
    "spcr-settings": "^1.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/mousetrap": "^1.6.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "lefthook": "^1.11.13",
    "spicetify-creator": "^1.0.17"
  },
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
